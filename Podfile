# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'ProjectMobil' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
pod 'Alamofire'
pod 'GoogleMaps'
pod 'GooglePlaces'
pod 'SwiftyJSON'

  # Pods for ProjectMobil

  target 'ProjectMobilTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ProjectMobilUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
